@echo off

REM Node 1 - modem, Netfoss FOSSIL real COM4, WCIRQID=4 WCBASEID=$02F8

SET WCNODEID=1
SET WCPORTID=2

REM Tradewars
SET TWNODE=1

REM Netfoss node 1, COM4
SET NFNODE=1
SET NFPORT=4

REM US Robotics MDM profile w/ FOSSIL enabled
SET WCMDM=USRDSF

REM Load FOSSIL driver in COM port mode
C:\NETFOSS\NETFOSS /N%WCNODEID% /C%NFPORT%

CALL cmncat.bat

REM Unload FOSSIL driver, release COM port
C:\NETFOSS\NETFOSS /U

echo This was WCNODEID %WCNODEID% NFPORT %NFPORT%
